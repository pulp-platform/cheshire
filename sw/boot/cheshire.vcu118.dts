/include/ "cheshire.dtsi"

&spi {
  boot-with = <1>;
  nor@1 {
    // Note : u-boot does not find mt25qu02g
    compatible = "mt25qu01g", "jedec,spi-nor";
    reg = <0x1>; // CS
    spi-max-frequency = <25000000>;
    disable-wp;
  };
};
